############read E I R
record(ai, "$(Sys)$(Dev)Val:RB-E"){
        field(INP,"@K2611.proto SourMeasV() $(PORT)")  
        field(EGU, "Volts")
	field(SCAN,"1 second")
        field(DTYP,"stream")
}

record(ai, "$(Sys)$(Dev)Val:RB-I"){         
        field(INP,"@K2611.proto SourMeasI() $(PORT)")   
        field(EGU, "Amps")      
        field(DTYP,"stream")
        field(SCAN, "I/O Intr")
} 

record(ai, "$(Sys)$(Dev)Val:RB-R"){         
        field(INP,"@K2611.proto SourMeasR() $(PORT)")   
        field(EGU, "Ohms")      
        field(DTYP,"stream") 
	field(SCAN, "I/O Intr")
}


