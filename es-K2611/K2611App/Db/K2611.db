#record(stringin, "$(Sys)$(Dev)Val:RB-ESP"){
record(ai, "$(Sys)$(Dev)Val:RB-ESP"){
        field(INP,"@K2611.proto GetVsp() $(PORT)")  #INP means input?
        field(EGU, "Volts")                       #EGU is engineering units  (EGUF - full scale, EGUL low scale)
        field(SCAN,"1 second")
        field(DTYP,"stream")  
}
 
 
